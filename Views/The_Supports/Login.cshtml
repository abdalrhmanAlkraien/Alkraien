@model adminSection.Models.The_Supports
@{
    ViewBag.Title = "Login";
    Layout = "~/Views/Shared/_LoginLayoutPage1.cshtml";
}
<style>
    .bu{
        padding:10px;
        margin:10px;
    }

</style>
<h2>login</h2>
<div class="form-horizontal">

    @using (Html.BeginForm())
    {
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            @Html.LabelFor(x => x.Supports_Email, htmlAttributes: new { @class = "label-from col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(x => x.Supports_Email, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(x => x.Supports_Email, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(x => x.Supports_Password, htmlAttributes: new { @class = "label-from col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(x => x.Supports_Password, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(x => x.Supports_Password, "", new { @class = "text-danger" })
            </div>
        </div>


        <div class="form-group">
            <div class="col-md-2">
                <input type="submit" value="Login" class="btn btn-primary bu" />
            </div>

        </div>

    }



</div>


